<div class="container">
    <div id="yield">
    <dl>
      <% @expos.each do |expo| %>
        <dt> <%= link_to expo.title, user_expo_path(expo.user.id, expo.id)%>  by: <%=expo.user.name%>
        </dt>
          <p class="time">Created <%= time_ago_in_words(expo.created_at)%> ago</p>
          <br>
      <% end %>
    </dl>
  </div>
</div>
