<%= form_for [@user, @expo] do |form| %>
  <% form.label :title %>
    <p><%= form.text_area :title, placeholder: "Title", :cols => "70", :rows => "1" %></p>
  <% form.label :body %>
    <p><%= form.text_area :body, placeholder: "Ideas", :cols => "70", :rows => "10" %></p>
  <% form.label :art %>
    <p><%= form.file_field :art%></p>
  <% form.label :url %>
    <p><%= form.text_area :url, placeholder: "Artifact Link", :cols => "70", :rows => "1" %></p>
  <% form.label :tag %>
    <p><%= form.text_area :tag, placeholder: "Tag", :cols => "70", :rows => "1" %></p>
    <p><%= form.submit %></p>
<% end %>
